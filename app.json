{
  "expo": {
    "name": "Eco Natureza",
    "slug": "econatureza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#885500"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "V-1",
    "githubUrl": "https://github.com/juliandror/econatureza",
    "android": {
      "package": "com.juliandrorocha.ecocerrado",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "LOCATION"
      ]
    }
  }
}
